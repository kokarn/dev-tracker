server {
    listen 80 default_server;
    listen [::]:80 default_server ipv6only=on;

    return 301 https://$host$request_uri;
}

server {
    server_name www.arkdevtracker.com;
    return 301 https://arkdevtracker.com$request_uri;
}

server {
    server_name www.battlefielddevtracker.com;
    return 301 https://battlefielddevtracker.com$request_uri;
}

